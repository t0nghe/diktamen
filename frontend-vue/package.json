  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^18.6.4",
    "@vitejs/plugin-vue": "^3.0.0",
    "@vitest/ui": "^0.21.0",
